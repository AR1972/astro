NUL		equ	0		; 00   ^@   Null
BEL		equ	7		; 07   ^G   Bell
BS		equ	8		; 08   ^H   Backspace
TAB		equ	9		; 09   ^I   (Horizontal) Tab
LF		equ	10		; 0A   ^J   Line Feed
FF		equ	12		; 0C   ^L   Form Feed
CR		equ	13		; 0D   ^M   Carriage Return
ESCAPE		equ	27		; 1B   ^[   Escape
SPACE		equ	32		; 20        Space
DEL		equ	127		; 7F        Delete/Rubout

