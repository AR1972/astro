
; ----------------------------------------------------------

        PUBLIC  $M_CLS_4
        IF1
        %out    ... Including message Class D
        ENDIF

; ----------------------------------------------------------

$M_CLASS_D_STRUC LABEL BYTE
        $M_CLASS_ID <0FFH,EXPECTED_VERSION,Class_D_MessageCount>

; ----------------------------------------------------------


$M_D_0001BH_STRUC LABEL BYTE
        $M_ID   <0001BH,$M_D_0001BH_MSG-$M_D_0001BH_STRUC>

; ----------------------------------------------------------


$M_D_0001BH_MSG LABEL BYTE
        DB      $M_D_0001BH_END-$M_D_0001BH_MSG-1
        DB      "Name of list device [PRN]: "
$M_D_0001BH_END LABEL BYTE
  
; ----------------------------------------------------------

Class_D_MessageCount EQU     1

; ----------------------------------------------------------

        IF      FARmsg
$M_CLS_4 PROC FAR
        ELSE
$M_CLS_4 PROC NEAR
        ENDIF

        PUSH    CS
        POP     ES
        LEA     DI,$M_CLASS_D_STRUC
        ADD     CX,$-$M_CLASS_D_STRUC
        RET

$M_CLS_4 ENDP

